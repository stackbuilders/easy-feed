<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{ title }}</title>
  {{# author }}
  <author>
    <name>{{ name }}</name>
    {{# email }}
    <email>{{ . }}</email>
    {{/ email }}
    {{# uri }}
    <uri>{{ . }}</uri>
    {{/ uri }}
  </author>
  {{/ author }}
  {{# category }}
  <category>
    <term>{{ term }}</term>
    {{# scheme }}
    <scheme>{{ . }}</scheme>
    {{/ scheme }}
    {{# label }}
    <label>{{ . }}</label>
    {{/ label }}
  </category>
  {{/ category }}
  {{# contributor }}
  <contributor>
    <name>{{ name }}</name>
    {{# email }}
    <email>{{ . }}</email>
    {{/ email }}
    {{# uri }}
    <uri>{{ . }}</uri>
    {{/ uri }}
  </contributor>
  {{/ contributor }}
  {{# generator }}
  <generator{{# uri }} uri="{{ uri }}"{{/ uri }}{{# version }} version="{{ version }}"{{/ version }}>
    {{ text }}
  </generator>
  {{/ generator }}
  {{# icon }}
  <icon>{{ . }}</icon>
  {{/ icon }}
  <id>{{ id }}</id>
  {{# link }}
  <link href="{{ href }}"{{# rel }}
   rel="{{ . }}"{{/ rel }}{{# type }}
   type="{{ . }}"{{/ type }}{{# hreflang }}
   hreflang="{{ . }}"{{/ hreflang }}{{# title }}
   title="{{ . }}"{{/ title }}{{# length }}
   length="{{ . }}"{{/ length }}/>
  {{/ link }}
  {{# logo }}
  <logo>{{ . }}</logo>
  {{/ logo }}
  {{# rights }}
  <rights>{{ . }}</rights>
  {{/ rights }}
  {{# subtitle }}
  <subtitle>{{ . }}</subtitle>
  {{/ subtitle }}
  {{# updated }}
  <updated>{{ . }}</updated>
  {{/ updated }}
</feed>
